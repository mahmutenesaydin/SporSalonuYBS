@using SporsalonuBYSS.Models
@model Sponsor
@{
    ViewBag.Title = "RemoveSponsor";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<form method="post" action="/AdminipSponsor/RemoveSponsor">
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Ürünler</h6>
        </div>
        <div class="card-body">
            <p>@Model.SponsorName isimli sponsor silinsin mi ?</p>
            <input type="hidden" name="SponsorID" value="@Model.SponsorID" />
            <button type="submit" class="btn btn-dark">Sil</button>
        </div>
    </div>
</form>
