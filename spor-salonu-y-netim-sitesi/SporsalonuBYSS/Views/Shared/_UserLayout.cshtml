
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="icon" href="~/Content/UserContent/img/fav-icon.png" type="image/x-icon" />
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>STORM GYM</title>

    <!-- Icon css link -->
    <link href="~/Content/UserContent/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/UserContent/vendors/line-icon/css/simple-line-icons.css" rel="stylesheet">
    <link href="~/Content/UserContent/vendors/elegant-icon/style.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="~/Content/UserContent/css/bootstrap.min.css" rel="stylesheet">

    <!-- Rev slider css -->
    <link href="~/Content/UserContent/vendors/revolution/css/settings.css" rel="stylesheet">
    <link href="~/Content/UserContent/vendors/revolution/css/layers.css" rel="stylesheet">
    <link href="~/Content/UserContent/vendors/revolution/css/navigation.css" rel="stylesheet">

    <!-- Extra plugin css -->
    <link href="~/Content/UserContent/vendors/owl-carousel/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/UserContent/vendors/bootstrap-selector/css/bootstrap-select.min.css" rel="stylesheet">

    <link href="~/Content/UserContent/css/style.css" rel="stylesheet">
    <link href="~/Content/UserContent/css/responsive.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <!--================Top Header Area =================-->
    <div class="header_top_area">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">

                </div>
                <div class="col-lg-6">
                    <div class="top_header_middle">
                        <img src="~/Content/UserContent/img/storm_gym_logo.png" alt="" width="350" height="110">
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="top_right_header">
                        <br><br><br><br>
                        <ul class="top_right">
                           
                            @{
                                Html.RenderAction("BasketWidget", "UserpProducts");
                            }
                                
                            <li class="user">
                                <a href="#" class="nav-link px-lg-4 dropdown" data-toggle="dropdown">
                                    @User.Identity.Name
                                    <i class="icon-user icons" style="color:#ccc" aria-hidden="true"></i>
                                </a>
                                <div class="dropdown-menu">
                                    <a class="dropdown-item" href="/Member/UpdateUser">Bilgilerimi Güncelle</a>
                                    <a class="dropdown-item" href="/Member/SignOut">Çıkış Yap</a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--================End Top Header Area =================-->
    <!--================Menu Area =================-->
    <header class="shop_header_area">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <a class="navbar-brand" href="~/Content/UserContent/#"><img src="~/Content/UserContent/img/logo.png" alt=""></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">

                    <ul class="navbar-nav">
                        &emsp;&emsp;
                        <li class="nav-item dropdown submenu">
                            <a class="nav-link dropdown-toggle" href="~/Content/UserContent/#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                Kategoriler <i class="fa fa-angle-down" aria-hidden="true"></i>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="nav-item"><a class="nav-link" href="@Url.Action("GetUserProducts","UserpProducts")">Protein Tozu</a></li>
                                <li class="nav-item"><a class="nav-link" href="@Url.Action("GetUserGainer","UserpProducts")">Gainerlar</a></li>
                                <li class="nav-item"><a class="nav-link" href="@Url.Action("GetUserBCAA","UserpProducts")">BCAA</a></li>
                            </ul>
                        </li>
                    </ul>
                    &emsp;&emsp;&emsp;&emsp;
                    <ul class="navbar-nav categories">
                        <li class="nav-item">
                        </li>
                    </ul>
                    <ul class="navbar-nav">
                        <li class="nav-item"><a class="nav-link" href="~/Home/UserIndex">Anasayfa</a></li>

                        <li class="nav-item dropdown submenu">
                            <a class="nav-link dropdown-toggle" href="" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                Hakkımızda <i class="fa fa-angle-down" aria-hidden="true"></i>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="nav-item"><a class="nav-link" href="~/UserpInformation/GetInformation">Antrenörlerimiz</a></li>
                                <li class="nav-item"><a class="nav-link" href="~/UserpSponsor/ListSponsor">Sponsorlarımız</a></li>
                            </ul>
                        </li>
                        
                        <li class="nav-item"><a class="nav-link" href="~/Home/GetContact/#">İletişim</a></li>

                    </ul>
                </div>
            </nav>
        </div>
    </header>
    <!--================End Menu Area =================-->
    <!--================Slider Area =================-->
    <section class="main_slider_area">
        <div class="container">
            @RenderBody()

        </div>
    </section>
    <!--================End Form Blog Area =================-->
    <!--================Footer Area =================-->
    <footer class="footer_area">
        <div class="container">
            <div class="footer_widgets">
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-6">
                        <aside class="f_widget f_about_widget">
                            <img src="~/Content/UserContent/img/storm_gym_logo.png" alt="" width="250" height="79">

                        </aside>
                    </div>
                    <div class="col-lg-2 col-md-4 col-6">
                        <aside class="f_widget link_widget f_info_widget">
                            <div class="f_w_title">
                                <h3>Hakkımızda</h3>
                            </div>
                            <ul>
                                
                                <li><a href="~/UserpSponsor/ListSponsor">Sponsorlarımız</a></li>
                                <li><a href="~/UserpInformation/GetInformation">Antrenörler</a></li>
                            </ul>
                        </aside>
                    </div>
                    <div class="col-lg-2 col-md-4 col-6">

                    </div>
                    <div class="col-lg-2 col-md-4 col-6">

                    </div>
                    <div class="col-lg-2 col-md-4 col-6">
                        <aside class="f_widget link_widget f_account_widget">
                            <div class="f_w_title">
                                <h3>Hesabım</h3>
                            </div>
                            <ul>
                                <li>@Session["Membername"]</li>
                                <li><a href="~/Member/UpdateUser">Hesap Ayarlarım</a></li>
                                <li><a href="~/UserpOrder_Details/OrderDetailsIndex">Siparişlerim</a></li>
                            </ul>
                        </aside>
                    </div>
                </div>
            </div>
            <div class="footer_copyright">
                <h5>
                    ©
                    <script>document.write(new Date().getFullYear());</script> <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    Copyright &copy;
                    <script>document.write(new Date().getFullYear());</script> All rights reserved | STORM GYM
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </h5>
            </div>
        </div>
    </footer>
    <!--================End Footer Area =================-->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/Content/UserContent/js/jquery-3.2.1.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Content/UserContent/js/popper.min.js"></script>
    <script src="~/Content/UserContent/js/bootstrap.min.js"></script>
    <!-- Rev slider js -->
    <script src="~/Content/UserContent/vendors/revolution/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/Content/UserContent/vendors/revolution/js/jquery.themepunch.revolution.min.js"></script>
    <script src="~/Content/UserContent/vendors/revolution/js/extensions/revolution.extension.actions.min.js"></script>
    <script src="~/Content/UserContent/vendors/revolution/js/extensions/revolution.extension.video.min.js"></script>
    <script src="~/Content/UserContent/vendors/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script src="~/Content/UserContent/vendors/revolution/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script src="~/Content/UserContent/vendors/revolution/js/extensions/revolution.extension.navigation.min.js"></script>
    <script src="~/Content/UserContent/vendors/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <!-- Extra plugin css -->
    <script src="~/Content/UserContent/vendors/counterup/jquery.waypoints.min.js"></script>
    <script src="~/Content/UserContent/vendors/counterup/jquery.counterup.min.js"></script>
    <script src="~/Content/UserContent/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Content/UserContent/vendors/bootstrap-selector/js/bootstrap-select.min.js"></script>
    <script src="~/Content/UserContent/vendors/image-dropdown/jquery.dd.min.js"></script>
    <script src="~/Content/UserContent/js/smoothscroll.js"></script>
    <script src="~/Content/UserContent/vendors/isotope/imagesloaded.pkgd.min.js"></script>
    <script src="~/Content/UserContent/vendors/isotope/isotope.pkgd.min.js"></script>
    <script src="~/Content/UserContent/vendors/magnify-popup/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/UserContent/vendors/vertical-slider/js/jQuery.verticalCarousel.js"></script>
    <script src="~/Content/UserContent/vendors/jquery-ui/jquery-ui.js"></script>

    <script src="~/Content/UserContent/js/theme.js"></script>
</body>
</html>